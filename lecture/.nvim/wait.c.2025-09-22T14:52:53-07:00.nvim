#include <unistd.h>
#include <stdio.h>
#include <sys/types.h>
#include <sys/wait.h>

int main() {
  pid_t pid = fork();
  if (pid == 0) {
    execl("/bin/ls", "/bin/ls", "-a","-l",NULL);
  } else {
    if (pid) {
      int wstatus = 0;
      if (waitpid(pid, &wstatus, 0) == 0) {
        if (WIFEXITED(wstatus)) {
          printf("Status: %d\n", WEXITSTATUS(wstatus));
        }
      }
    }
  }
}
