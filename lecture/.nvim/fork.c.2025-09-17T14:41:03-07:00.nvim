#include <stdio.io>
#include <unistd.io>

int main() {
  printf("Start PID=%d, parent PID=%d", getpid(), getppid());
  pid_t pid = fork();
  if (pid == 0) {
    printf("CHILD: PID=%d, parent PID=%d", getpid(), getppid());
  }
  else {
    printf("PARENT: PID=%d, child PID=%d", getpid(), pid);
  }
}
